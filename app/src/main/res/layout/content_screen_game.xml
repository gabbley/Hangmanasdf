<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.gabbley.hangman.screenGame"
    tools:showIn="@layout/activity_screen_game">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A"
        android:id="@+id/btnA"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        android:id="@+id/btnB"
        android:layout_above="@+id/btnH"
        android:layout_toEndOf="@+id/btnA" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
        android:id="@+id/btnC"
        android:layout_alignTop="@+id/btnB"
        android:layout_toEndOf="@+id/btnB" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D"
        android:id="@+id/btnD"
        android:layout_alignBottom="@+id/btnC"
        android:layout_toEndOf="@+id/btnC" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E"
        android:id="@+id/btnE"
        android:layout_alignBottom="@+id/btnD"
        android:layout_toEndOf="@+id/btnD" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="F"
        android:id="@+id/btnF"
        android:layout_alignTop="@+id/btnE"
        android:layout_toEndOf="@+id/btnE" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="G"
        android:id="@+id/btnG"
        android:layout_alignBottom="@+id/btnF"
        android:layout_toEndOf="@+id/btnF" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="H"
        android:id="@+id/btnH"
        android:layout_below="@+id/btnA"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I"
        android:id="@+id/btnI"
        android:layout_alignBottom="@+id/btnH"
        android:layout_toEndOf="@+id/btnH" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="J"
        android:id="@+id/btnJ"
        android:layout_alignBottom="@+id/btnI"
        android:layout_alignStart="@+id/btnC" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K"
        android:id="@+id/btnK"
        android:layout_below="@+id/btnD"
        android:layout_toStartOf="@+id/btnE" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="L"
        android:id="@+id/btnL"
        android:layout_alignBottom="@+id/btnK"
        android:layout_alignStart="@+id/btnE" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="M"
        android:id="@+id/btnM"
        android:layout_below="@+id/btnF"
        android:layout_toStartOf="@+id/btnG" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="O"
        android:id="@+id/btnO"
        android:layout_below="@+id/btnH"
        android:layout_toStartOf="@+id/btnI" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="P"
        android:id="@+id/btnP"
        android:layout_alignTop="@+id/btnQ"
        android:layout_toEndOf="@+id/btnH" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="R"
        android:id="@+id/btnR"
        android:layout_alignTop="@+id/btnQ"
        android:layout_toEndOf="@+id/btnX" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Q"
        android:id="@+id/btnQ"
        android:layout_below="@+id/btnI"
        android:layout_toEndOf="@+id/btnI" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="S"
        android:id="@+id/btnS"
        android:layout_alignTop="@+id/btnT"
        android:layout_toStartOf="@+id/btnT" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="T"
        android:id="@+id/btnT"
        android:layout_above="@+id/btnZ"
        android:layout_toStartOf="@+id/btnG" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="U"
        android:id="@+id/btnU"
        android:layout_alignTop="@+id/btnT"
        android:layout_alignStart="@+id/btnG" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="V"
        android:id="@+id/btnV"
        android:layout_below="@+id/btnP"
        android:layout_toStartOf="@+id/btnP" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="W"
        android:id="@+id/btnW"
        android:layout_below="@+id/btnP"
        android:layout_alignStart="@+id/btnP" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="X"
        android:id="@+id/btnX"
        android:layout_below="@+id/btnQ"
        android:layout_toStartOf="@+id/btnK" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Y"
        android:id="@+id/btnY"
        android:layout_below="@+id/btnR"
        android:layout_alignStart="@+id/btnR"
        android:layout_toStartOf="@+id/btnS" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Z"
        android:id="@+id/btnZ"
        android:layout_alignTop="@+id/btnY"
        android:layout_toEndOf="@+id/btnY" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RESET"
        android:id="@+id/buttonReset"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/btnY" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="N"
        android:id="@+id/btnN"
        android:layout_alignTop="@+id/btnK"
        android:layout_alignEnd="@+id/btnU"
        android:layout_alignStart="@+id/btnG" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/btnV">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="214dp"
            android:layout_row="9"
            android:layout_column="14"></TableRow>
    </GridLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/buttonReset"
        android:layout_alignParentEnd="true">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="213dp"
            android:layout_gravity="center_horizontal|bottom">

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView"
                android:layout_row="0"
                android:layout_column="0"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView2"
                android:layout_row="0"
                android:layout_column="1"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView3"
                android:layout_row="0"
                android:layout_column="2"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView4"
                android:layout_row="0"
                android:layout_column="3"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView5"
                android:layout_row="0"
                android:layout_column="4"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView6"
                android:layout_row="0"
                android:layout_column="5"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView7"
                android:layout_row="0"
                android:layout_column="6"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView8"
                android:layout_row="0"
                android:layout_column="7"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView9"
                android:layout_row="0"
                android:layout_column="8"
                android:singleLine="false" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView10"
                android:layout_row="0"
                android:layout_column="9"
                android:singleLine="false" />

        </GridLayout>
    </FrameLayout>

</RelativeLayout>
